git clone <repository-url>
cd user-management

cd server
npm install

# create .env file inside server folder and add:
PORT=5000
MONGO_URI=your_mongodb_connection_string
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES=1d
CLIENT_URL=frontend_url

npm run dev

cd ../client
npm install

# create .env file inside client folder and add:
VITE_API_URL=server_url

npm run dev